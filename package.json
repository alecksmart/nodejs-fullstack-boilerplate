    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.2",
    "sequelize-cli": "^5.5.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  }
