    "apollo-boost": "^0.4.4",
    "apollo-server-express": "^2.9.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "mysql2": "^1.7.0",
    "sequelize": "^5.18.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^4.1.2",
